<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_temptable_local"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="50dp"/>

    <TextView
        android:id="@+id/textView_elderID_temptable_local"
        android:layout_width="48dp"
        android:layout_height="wrap_content"
        android:layout_marginVertical="5dp"
        android:layout_marginStart="3dp"
        android:text="TextView"
        android:maxLines="1"
        android:singleLine="true"
        android:textSize="20sp"
        android:layout_marginHorizontal="3dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

        <TextView
            android:layout_marginHorizontal="5dp"
            android:id="@+id/textView_temp_temptable_local"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:layout_marginVertical="5dp"
            android:maxLines="1"
            android:singleLine="true"
            android:text="TextView"
            android:textSize="20sp"
            app:layout_constraintStart_toEndOf="@id/guideline_temptable_local"
            app:layout_constraintTop_toTopOf="parent"/>

<!--    <TextView-->
<!--        android:id="@+id/textView_deviceID_temptable_local"-->
<!--        android:layout_width="85dp"-->
<!--        android:layout_height="wrap_content"-->
<!--        android:layout_marginVertical="5dp"-->
<!--        android:layout_marginHorizontal="8dp"-->
<!--        android:maxLines="1"-->
<!--        android:singleLine="true"-->
<!--        android:text="TextView"-->
<!--        android:textSize="20sp"-->
<!--        app:layout_constraintStart_toEndOf="@id/guideline_temptable_local"-->
<!--        app:layout_constraintTop_toTopOf="parent"/>-->

        <TextView
            android:id="@+id/textView_timestamp_temptable_local"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="5dp"
            android:layout_marginStart="3dp"
            android:layout_marginEnd="3dp"
            android:maxLines="1"
            android:singleLine="true"
            android:text="TextView"
            android:textSize="18sp"
            app:layout_constraintStart_toEndOf="@id/textView_temp_temptable_local"
            app:layout_constraintTop_toTopOf="parent"
            />

    <TextView
        android:id="@+id/textView_status_temptable_local"
        android:layout_width="25dp"
        android:layout_height="wrap_content"
        android:layout_marginVertical="5dp"
        android:layout_marginStart="3dp"
        android:maxLines="1"
        android:singleLine="true"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />


    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="?android:attr/listDivider"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>